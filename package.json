{
  "name": "vite-plugin-build-info",
  "version": "0.0.0",
  "description": "add build info to your vite project",
  "keywords": [
    "plugin",
    "vite"
  ],
  "author": "OriLight <orilights@163.com>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OriLight152/vite-plugin-build-info.git"
  },
  "homepage": "https://github.com/OriLight152/vite-plugin-version-mark.git",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
